{
  "id": null,
  "title": "Business Metrics Dashboard",
  "tags": ["business", "kpi", "analytics"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "User Activity Overview",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Active Users",
      "type": "stat",
      "targets": [
        {
          "expr": "active_users",
          "legendFormat": "Currently Active"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 1}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 1
      }
    },
    {
      "id": 3,
      "title": "Daily Active Users",
      "type": "stat",
      "targets": [
        {
          "expr": "daily_active_users",
          "legendFormat": "DAU"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "green", "value": 50}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 1
      }
    },
    {
      "id": 4,
      "title": "Monthly Active Users",
      "type": "stat",
      "targets": [
        {
          "expr": "monthly_active_users",
          "legendFormat": "MAU"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "green", "value": 500}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 1
      }
    },
    {
      "id": 5,
      "title": "User Registrations",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(user_registration_total[24h])",
          "legendFormat": "Registrations/Hour"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 0}
            ]
          },
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 1
      }
    },
    {
      "id": 6,
      "title": "Transaction Analytics",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 9
      }
    },
    {
      "id": 7,
      "title": "Transaction Volume (24h)",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(transaction_volume_total{status=\"success\"}[24h])",
          "legendFormat": "{{transaction_type}}"
        }
      ],
      "yAxes": [
        {
          "label": "Volume/Hour",
          "unit": "currencyUSD"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 10
      }
    },
    {
      "id": 8,
      "title": "Transaction Success Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "transaction_success_rate",
          "legendFormat": "{{transaction_type}}"
        }
      ],
      "yAxes": [
        {
          "label": "Success Rate",
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 10
      }
    },
    {
      "id": 9,
      "title": "Total Balance",
      "type": "stat",
      "targets": [
        {
          "expr": "balance_total",
          "legendFormat": "Total Balance"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 0}
            ]
          },
          "unit": "currencyUSD"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 18
      }
    },
    {
      "id": 10,
      "title": "Cache Hit Ratio",
      "type": "stat",
      "targets": [
        {
          "expr": "cache_hit_ratio",
          "legendFormat": "Hit Ratio"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "green", "value": 90}
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 18
      }
    },
    {
      "id": 11,
      "title": "System Health",
      "type": "stat",
      "targets": [
        {
          "expr": "system_health",
          "legendFormat": "{{component}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          },
          "mappings": [
            {
              "options": {
                "0": {"text": "Unhealthy"},
                "1": {"text": "Healthy"}
              },
              "type": "value"
            }
          ]
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 18
      }
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "refresh": "30s",
  "annotations": {
    "list": []
  },
  "templating": {
    "list": []
  }
} 